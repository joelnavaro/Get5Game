<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FinalActivity">

    <TextView
        android:id="@+id/c5_winRv"
        android:layout_width="28dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="end"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c5_win_Iv"
        app:layout_constraintEnd_toEndOf="@+id/c5_win_Iv" />

    <TextView
        android:id="@+id/c5_winTv"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="bottom"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c5_win_Iv"
        app:layout_constraintStart_toStartOf="@+id/c5_win_Iv" />

    <TextView
        android:id="@+id/c4_winTv"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="bottom"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c4_win_Iv"
        app:layout_constraintStart_toStartOf="@+id/c4_win_Iv" />

    <TextView
        android:id="@+id/c4_winRv"
        android:layout_width="28dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="end"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c4_win_Iv"
        app:layout_constraintEnd_toEndOf="@+id/c4_win_Iv" />

    <TextView
        android:id="@+id/c3_winTv"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="bottom"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c3_win_Iv"
        app:layout_constraintStart_toStartOf="@+id/c3_win_Iv" />

    <TextView
        android:id="@+id/c3_winRv"
        android:layout_width="28dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="end"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c3_win_Iv"
        app:layout_constraintEnd_toEndOf="@+id/c3_win_Iv" />

    <TextView
        android:id="@+id/c1_winTv"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="bottom"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c1_win_Iv"
        app:layout_constraintStart_toStartOf="@+id/c1_win_Iv" />

    <TextView
        android:id="@+id/c1_winRv"
        android:layout_width="28dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="end"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c1_win_Iv"
        app:layout_constraintEnd_toEndOf="@+id/c1_win_Iv"
        app:layout_constraintHorizontal_bias="0.187" />

    <TextView
        android:id="@+id/c2_winTv"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="bottom"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c2_win_Iv"
        app:layout_constraintStart_toStartOf="@+id/c2_win_Iv" />

    <TextView
        android:id="@+id/c2_winRv"
        android:layout_width="28dp"
        android:layout_height="34dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:gravity="end"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/c2_win_Iv"
        app:layout_constraintEnd_toEndOf="@+id/c2_win_Iv" />

    <Button
        android:id="@+id/finishBttn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="16dp"
        android:text="Finish"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/winner_txtView"
        android:layout_width="221dp"
        android:layout_height="62dp"
        android:layout_marginTop="100dp"
        android:gravity="center"
        android:text="WINNER!!"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_txtView"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Name:"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/c2_win_Iv"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/c5_win_Iv"
        app:layout_constraintTop_toBottomOf="@+id/c1_win_Iv" />

    <TextView
        android:id="@+id/winNameTv"
        android:layout_width="111dp"
        android:layout_height="66dp"
        android:layout_marginTop="16dp"
        android:text="name here"
        android:textSize="25sp"
        app:layout_constraintEnd_toStartOf="@+id/c2_win_Iv"
        app:layout_constraintStart_toEndOf="@+id/c5_win_Iv"
        app:layout_constraintTop_toBottomOf="@+id/name_txtView" />

    <ImageView
        android:id="@+id/c1_win_Iv"
        android:layout_width="80dp"
        android:layout_height="110dp"
        android:layout_marginTop="44dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/winner_txtView"
        app:layout_constraintVertical_bias="0.011"
        app:srcCompat="@drawable/backside" />

    <ImageView
        android:id="@+id/c2_win_Iv"
        android:layout_width="80dp"
        android:layout_height="110dp"
        android:layout_marginStart="46dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/c1_win_Iv"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.528"
        app:srcCompat="@drawable/backside" />

    <ImageView
        android:id="@+id/c3_win_Iv"
        android:layout_width="80dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.74"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c2_win_Iv"
        app:layout_constraintVertical_bias="0.293"
        app:srcCompat="@drawable/backside" />

    <ImageView
        android:id="@+id/c4_win_Iv"
        android:layout_width="80dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/c3_win_Iv"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c5_win_Iv"
        app:layout_constraintVertical_bias="0.293"
        app:srcCompat="@drawable/backside" />

    <ImageView
        android:id="@+id/c5_win_Iv"
        android:layout_width="80dp"
        android:layout_height="110dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="46dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/c1_win_Iv"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.528"
        app:srcCompat="@drawable/backside" />

</androidx.constraintlayout.widget.ConstraintLayout>